{
  "name": "waterdeliveryapp",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "start:clear": "expo start --dev-client --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "ios": "expo run:ios",
    "ios:clean": "cd ios && xcodebuild clean && cd ..",
    "web": "expo start --web",
    "lint": "expo lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "postinstall": "echo 'Skipping patch-package for build compatibility'",
    "optimize": "node ./scripts/build/optimize.js",
    "build:optimize": "npm run optimize && eas build -p android --profile production",
    "build:apk-optimized": "npm run optimize && eas build -p android --profile apk",
    "build:ultra": "npm run analyze && eas build -p android --profile apk-ultra",
    "analyze": "node ./scripts/analyze-bundle.js",
    "firebase:setup": "echo 'Please follow FIREBASE_SETUP.md for complete setup instructions'",
    "validate:firebase": "node -e \"console.log('Firebase packages:', Object.keys(require('./package.json').dependencies).filter(key => key.includes('firebase')))\"",
    "init:products": "node ./scripts/init-products.js",
    "init:data": "npm run init:products"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "2.1.2",
    "@react-native-community/cli-platform-android": "^18.0.0",
    "@react-native-community/cli-platform-ios": "^18.0.0",
    "@react-native-community/datetimepicker": "^8.4.1",
    "@react-native-firebase/app": "^20.5.0",
    "@react-native-firebase/auth": "^20.5.0",
    "@react-native-firebase/firestore": "^20.5.0",
    "@react-native-firebase/functions": "^20.5.0",
    "@react-native-firebase/storage": "^20.5.0",
    "@react-native-picker/picker": "^2.11.1",
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "^2.3.8",
    "@react-navigation/native": "^7.1.6",
    "@react-navigation/native-stack": "^7.3.10",
    "@reduxjs/toolkit": "^2.2.1",
    "@stripe/stripe-react-native": "0.45.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "expo": "53.0.13",
    "expo-blur": "~14.1.5",
    "expo-build-properties": "~0.14.6",
    "expo-constants": "~17.1.6",
    "expo-dev-client": "~5.2.2",
    "expo-font": "~13.3.1",
    "expo-haptics": "~14.1.4",
    "expo-image": "~2.3.0",
    "expo-linking": "~7.1.5",
    "expo-local-authentication": "~16.0.4",
    "expo-location": "~18.1.5",
    "expo-notifications": "~0.31.3",
    "expo-router": "~5.1.1",
    "expo-splash-screen": "^0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-symbols": "~0.4.5",
    "expo-system-ui": "~5.0.9",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.4",
    "react-native-biometrics": "^3.0.1",
    "react-native-calendars": "^1.1313.0",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-maps": "1.20.1",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-pdf": "^6.7.4",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-svg": "15.11.2",
    "react-native-web": "~0.20.0",
    "react-native-webview": "13.13.5",
    "react-redux": "^9.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@react-native-community/cli": "^18.0.0",
    "@testing-library/react-native": "^12.4.3",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~9.2.0",
    "firebase-admin": "^13.4.0",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.7",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0"
  },
  "private": true,
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    },
    "android": {
      "buildType": "apk",
      "backgroundColor": "#ffffff",
      "package": "com.waterdelivery.app"
    },
    "experiments": {
      "thinningWebpack": true,
      "optimizeJsLoadingProfiles": true,
      "turboModules": true
    },
    "packagerOpts": {
      "config": "metro.config.js",
      "sourceExts": [
        "js",
        "jsx",
        "ts",
        "tsx",
        "json"
      ]
    }
  },
  "resolutions": {
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "@react-native-async-storage/async-storage": "2.1.2"
  }
}
